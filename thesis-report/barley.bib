	doi = {10.1002/mnfr.201600238},
	file = {:C$\backslash$:/Users/tuhu/Downloads/Bresciani{\_}et{\_}al-2016-Molecular{\_}Nutrition{\_}{\&}{\_}Food{\_}Research.pdf:pdf},
	issn = {1613-4125},
	journal = {MOLECULAR NUTRITION {\&} FOOD RESEARCH},
